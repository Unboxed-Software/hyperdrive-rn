{
  "expo": {
    "name": "Clio",
    "slug": "sol-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sol-tracker",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/Splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beunboxed.sol-tracker",
      "CFBundleURLTypes": [
        {
          "CFBundleURLSchemes": ["com.googleusercontent.apps.520104427709-jp0dp1ph0pmf5j0vvpv0fcat8koqi7rg"]
        }
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.beunboxed.sol-tracker",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f5dfc574-9ebf-411c-9ca2-57af94eb621e"
      },
      "oneSignalAppId": "65d8f7ae-8eb7-4ed1-ac06-4683f3bc44eb"
    }
  }
}
